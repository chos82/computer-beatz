<tr class="<%= cycle('odd', 'even') %>">
	<td><%=h user.login %></td>
	<td><%=h user.email %></td>
	<td><%= user.enabled ? 'yes' : 'no ' %></td>
	<td>
	<% unless user == current_user %>
	<% if user.enabled %>
	<%= link_to('disable', admin_user_path(user.id), :method => :delete) %>
	<% else %>
	<%= link_to('enable', enable_admin_user_path(user.id), :method => :put) %>
	<% end %>
	<% end %>
	</td>
	<td>
	<%= link_to('destroy', remove_admin_user_path(user.id), :method => :delete, :confirm => "Really DESTROY this user?" ) %>
	</td>
	<td><%= link_to 'edit roles', admin_user_roles_path(user) %></td>
</tr>
