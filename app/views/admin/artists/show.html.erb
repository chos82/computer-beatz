<p>
	<h3>Artist: <%= link_to (h(@artist.name),
						     {:controller => '/artists',
						      :action => 'show',
							  :id => @artist},
						     {:target => 'blank'}) %></h3>
</p>
<% if @report %>
<p>
	<%= link_to( h(@user.login), {:controller => '/users',
								  :action => 'show',
							      :id => @user.id},
							    {:target => 'blank'}) %>: <%= @report.status %>
</p>
<% end %>
<% unless @artist.www.blank? %>
<p>
	<%= link_to @artist.www, 'http://' + @artist.www, :target => 'blank' %>
</p>
<% end %>
<%= link_to 'Edit', edit_admin_artist_path(@artist) %> |
<%= link_to 'Back', admin_music_index_path %>
