<p class="center">
	MIxes tagged with <strong>"<%=h @tag %>"</strong>
</p>

<%= render :partial => 'shared/order_by' %>

<table class="list-entries">
	<% for mix in @mixes do -%>
	<%= render :partial => 'mix', :object => mix %>
	<% end -%>
</table>

<%= will_paginate @mixes, :inner_window => 1, :outer_window => 1, :id => @tag, :order => params[:order] %>

<div class="page-count">
	(Showing <%= @from %> - <%=  @to %> / <%= @total %>)
</div>