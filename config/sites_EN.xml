<social_sites xsi:noNamespaceSchemaLocation="social_bookmarks.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <site>
    <name>Buzz</name>
    <url>http://buzz.yahoo.com/submit?submitUrl={link}&amp;submitHeadline={title}&amp;submitSummary=</url>
    <img>buzz.png</img>
    <key>buzz_en</key>
  </site>   
  <site>
    <name>Del.icio.us</name>
    <url>http://del.icio.us/post?url={link}&amp;title={title}</url>
    <img>delicious.png</img>
    <key>delicious_en</key>
  </site>
  <site>
    <name>digg</name>
    <url>http://digg.com/submit?phase=2&amp;url={link}&amp;title={title}</url>
    <img>digg.png</img>
    <key>digg_en</key>
  </site>
  <site>
    <name>DZone</name>
    <url>http://www.dzone.com/links/add.html?description={title}&amp;url={link}&amp;title={title}</url>
    <img>dzone.png</img>
    <key>dzone_en</key>
  </site>
  <site>
    <name>Facebook</name>
    <url>http://www.facebook.com/sharer.php?u={link}</url>
    <img>facebook.png</img>
    <key>facebook_en</key>
  </site>
  <site>
    <name>FURL</name>
    <url>http://furl.net/storeIt.jsp?t={title}&amp;u={link}</url>
    <img>furl.png</img>
    <key>furl_en</key>
  </site>
  <site>
    <name>Google Bookmarks</name>
    <url>http://www.google.com/bookmarks/mark?op=edit&amp;output=popup&amp;bkmk={link}&amp;title={title}</url>
